	string[] all_lines = File.ReadAllLines(path_of_file);
	using(StreamWriter sw = new StreamWriter(path_of_file))
	{
		for (int current_line = 1; current_line < all_lines.Length; ++current_line)
		{
			if (current_line == line_to_edit)
			{
				sw.WriteLine(lineToWrite);
			}
			else
			{
				sw.WriteLine(lines[current_line - 1]);
			}
		}
	}
	
