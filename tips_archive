18. Read an Excel file using C#
	string con = @"Provider=Microsoft.Jet.OLEDB.4.0;Data Source=D:\temp\test.xls;Extended Properties='Excel 8.0;HDR=Yes;'";    
using(OleDbConnection connection = new OleDbConnection(con))
{
    connection.Open();
    OleDbCommand command = new OleDbCommand("select * from [Sheet1$]", connection); 
    using(OleDbDataReader dr = command.ExecuteReader())
    {
         while(dr.Read())
         {
             var row1Col0 = dr[0];
             Console.WriteLine(row1Col0);
         }
    }
}
	(高版本Microsoft.ACE.OLEDB.12.0;)
	
	using System.Microsoft.Office.Interop.Excel as Excel
	class ReadExcel{
		public Excel.Application app;
		public Excel.Workbook workbook;
		public Excel.Worksheet worksheet;
		public int rowNum;
		public int columnNum;
		public object [,] values;//这样能提高效率,不需要单个读取excel
		
		public ReadExcel(string excel_name)
		{
			app = new Excel.Application();
			workbook = app.Workbooks.Open(excel_name, 0, true);
			worksheet = (Excel.Worksheet)workbook.Worksheets.get_Item(1);
			Excel.Range range = worksheet.UsedRange;
			rowNum = range.Rows.Count;
			columnNum = range.Columns.Count;
			
			Excel.Range valueRange = worksheet.get_Range("A1", worksheet.Cells.SpecialCells(Microsoft.Office.Interop.Excel.XlCellType.xlCellTypeLastCell));
			values = (object[,]) valueRange.Value2;
		}
	}
	
